<div class="col-md-4">
  <h1 class="cover-heading"><%= title %></h1>
  <div class="row">
    <% tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
            <%= twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>